#include "../lib/ShaderAPI.h"
#include "../lib/Manifold.h"

shader apManifoldTransform
[[
    string help = "Transform given Manifold"
]]
(
    Manifold inputManifold = MANIFOLD_ZERO()
    [[ string label = "Input Manifold" ]],

    MANIFOLD_XFORM_PARAMS(),
    MANIFOLD_OUTPUT()
)

{
    resultManifold = inputManifold;

    MANIFOLD_TRANSFORM();
    MANIFOLD_SET_OUTPUTS();
}