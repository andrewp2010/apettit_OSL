#include "../lib/ShaderAPI.h"

shader apFloatMax
[[
    string help = "Returns max value between Input A and Input B"
]]
(
    float inputA = 0.0
    [[ string label = "Input A" ]],
    float inputB = 0.0
    [[ string label = "Input B" ]],

    output float resultFloat = 0.0
)

{
    resultFloat = max(inputA, inputB);
}